{
  "workbench.colorTheme": "Aura Dracula Spirit (Soft)",
  "workbench.activityBar.location": "top",
  "workbench.editor.showTabs": "single",
  "workbench.statusBar.visible": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "workbench.sideBar.location": "left",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.hidesExplorerArrows": true,
  "window.zoomLevel": 1,
  "window.titleBarStyle": "native",
  "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.lineHeight": 2.5,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.semi": true,
  "prettier.endOfLine": "lf",
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.printWidth": 100,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.suggest.enabled": true,
  "typescript.suggest.enabled": true,
  "editor.quickSuggestions": {
    "other": "on"
  },
  "editor.suggestOnTriggerCharacters": false,
  "editor.hover.enabled": false,
  "editor.links": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.parameterHints.enabled": false,
  "editor.occurrencesHighlight": "off",
  "editor.selectionHighlight": false,
  "editor.renderWhitespace": "none",
  "editor.renderLineHighlight": "none",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.mouseWheelZoom": true,
  "editor.smoothScrolling": true,
  "editor.guides.indentation": false,
  "editor.guides.highlightActiveIndentation": false,
  "editor.renderIndentGuides": false,
  "editor.renderIndentGuidesHighlight": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.autoSave": "afterDelay",
  "update.mode": "none",
  "extensions.ignoreRecommendations": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "tabnine.experimentalAutoImports": true,
  "mssql.enableRichExperiences": true,
  "tailwind-fold.autoFold": false,
  "codeium.enableCodeLens": false,
  "workbench.colorCustomizations": {
    "editorBracketMatch.border": "#00000000",
    "editorBracketMatch.background": "#00000000",
    "editorBracketHighlight.foreground1": "#00000000",
    "editorBracketHighlight.foreground2": "#00000000",
    "editorBracketHighlight.foreground3": "#00000000",
    "editorBracketHighlight.unexpectedBracket.foreground": "#00000000",
    "tab.border": "#00000000",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "editorGroup.border": "#00000000",
    "sideBar.border": "#00000000",
    "panel.border": "#00000000",
    "editor.lineHighlightBorder": "#00000000",
    "editorOverviewRuler.border": "#00000000",
    "editorGutter.border": "#00000000",
    "editorIndentGuide.background1": "#00000000",
    "editorIndentGuide.background2": "#00000000",
    "editorIndentGuide.background3": "#00000000",
    "editorIndentGuide.activeBackground1": "#00000000",
    "editorIndentGuide.activeBackground2": "#00000000",
    "editorIndentGuide.activeBackground3": "#00000000",
    "editorRuler.foreground": "#00000000",
    "editorCursor.background": "#000000",
    "[Aura Dracula Spirit (Soft)]": {
      "editorSuggestWidget.selectedBackground": "#3A334B",
      "sideBar.background": "#191521"
    }
  }
}
